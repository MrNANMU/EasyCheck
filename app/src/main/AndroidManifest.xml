<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dasong.easycheck">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".simple.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".simple.ui.ContextSimpleActivity">
        </activity>

        <activity android:name=".simple.ui.ObjectSimpleActivity">
        </activity>

        <activity android:name=".simple.ui.CheckerActivity">
        </activity>

        <activity android:name=".simple.ui.CallbackSimpleActivity">
        </activity>

        <activity android:name=".simple.ui.FilterSimpleActivity">
        </activity>
    </application>

</manifest>